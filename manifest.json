{
  "manifest_version": 3,
  "name": "Solglyph Airdrop Alert",
  "description": "Keep track and stay updated with the current airdrops out there",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}